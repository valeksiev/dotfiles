require 'spec_helper'

describe 'ETL' do

  include FixturesHelper

  it 'does magic' do
    connector = Mapper::FileSystemConnector.new(fixture_path('test.csv'))
    adapter = Mapper::CsvDataSourceAdapter.new connector
    op_formatter = Mapper::ConsoleOutputFormatter.new
    runner = Mapper::Runner.new adapter, op_formatter
    runner.run
  end

end
